{
  "manifest_version": 2,
  "version": "1.0.0",
  "author": "caibin",
  "name": "QrcodeMaker",
  "description": "An extension to make qrcode for current page's url",
  
  "browser_action": {
    "default_icon": "./images/16.png",
    "default_title": "二维码生成器",
    "default_popup": "main.html"
  },

  "icons": {
    "16": "./images/16.png",
    "48": "./images/48.png",
    "128": "./images/128.png"
  },

  "permissions": [
    "tabs"
  ],

  "content_security_policy": "script-src 'self'; object-src 'self'"

}